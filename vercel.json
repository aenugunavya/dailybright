{
  "version": 2,
  "framework": "nextjs",
  "buildCommand": "cd apps/web && npm install && npm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "cd apps/web && npm install",
  "crons": [
    {
      "path": "/api/cron/daily-prompt",
      "schedule": "37 9 * * *"
    },
    {
      "path": "/api/cron/daily-prompt",
      "schedule": "23 11 * * *"
    },
    {
      "path": "/api/cron/daily-prompt",
      "schedule": "51 14 * * *"
    },
    {
      "path": "/api/cron/daily-prompt",
      "schedule": "18 16 * * *"
    },
    {
      "path": "/api/cron/daily-prompt",
      "schedule": "44 18 * * *"
    }
  ]
}
